{
    "name": "savio.varsalle/laravel-correios-basico",
    "description": "Pacote para consulta de preços e prazos no site dos correios sem cartão de postagem.",
    "type": "library",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "SavioVarsalle\\LaravelCorreiosBasico\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Savio Varsalle",
            "email": "savio.varsalle@live.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/dom-crawler": "6.4"
    },
    "require-dev": {
        "php": ">=8.1",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "spatie/laravel-ignition": "^2.0",
        "pestphp/pest": "^2.35"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
